{
    "presets": ["next/babel"],
    "env": {
        "development": {
            "plugins": [
                [
                    "babel-plugin-styled-components",
                    {
                        "ssr": true
                    }
                ]
            ]
        },
        "test": {
            "presets": ["env", "next/babel"]
        }
    },
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "ssr": true
            }
        ],
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragmaFrag": "React.Fragment"
            }
        ]
    ]
}
